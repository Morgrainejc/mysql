# big countries
select name,population,area
from World
where population > 25000000 or area > 3000000

#swap salary
update salary
set sex = if(sex = 'f','m','f')

#not boring movie
select *
from cinema
where description !='boring' and (id%2) = 1
order by rating desc

#duplicate emails
select Email
from Person
group by Email
having count(*) >1 #where过滤整体，having过滤组，where必须在group by 之前

